#include <stdio.h>
#include <stdlib.h>
/* run this program using the console pauser or add your own getch, system("pause") or input loop */
int main(int argc, char *argv[]) {
	float x,y,z,t;
	printf("Vvedite x:\nx=");
	scanf("%f",&x);
	if(x <= 0){
		y = 0;
		printf("%.2f",y);
	}else{
		if((x <= 1)&&(x > 0)){
			z = x;
			printf("%.2f",z);
		}else{
			if(x > 1){
				t=x*x*x;
				printf("%.2f",t);
			}
		}
	}
	return 0;
}
